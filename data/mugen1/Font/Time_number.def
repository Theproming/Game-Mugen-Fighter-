; Enter 48 - 48 point font
; Indexed 8-bank
; Bank 0 - mono
; Bank 1 - red
; Bank 2 - green
; Bank 3 - blue
; Bank 4 - brown
; Bank 5 - cyan
; Bank 6 - purple
; Bank 7 - yellow

[FNT v2]
fntversion = 2,00
name = "Tekken 7 season 2 time font"

[Def]
Type = bitmap
BankType = palette
Size = 50,0
Spacing = -25,0
Offset = 0,0
File = Time_number.sff

; Note: All units are in pixels.
; Text rendered with bitmap fonts may be in ASCII only.
; See files in work/font for information on creating the sff.